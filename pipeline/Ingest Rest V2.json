			},
			"DestinationRawFile": {
				"type": "string",
				"defaultValue": "weather_daily_20221005.json"
			},
			"SourceSQL": {
				"type": "string",
				"defaultValue": "None"
			},
			"StatSQL": {
				"type": "string",
