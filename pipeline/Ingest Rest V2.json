				"defaultValue": 1
			},
			"DataToNumber": {
				"type": "int",
				"defaultValue": 1
			},
			"DataFormat": {
				"type": "string",
				"defaultValue": "None"
			},
			"SourceStructure": {
				"type": "string",
				"defaultValue": "None"
			},
			"MaxIntervalMinutes": {
				"type": "int",
				"defaultValue": 1
