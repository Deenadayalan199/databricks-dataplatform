			},
			"ActiveFlag": {
				"type": "bool",
				"defaultValue": true
			},
			"L1TransformationReqdFlag": {
				"type": "string",
				"defaultValue": "true"
			},
			"L2TransformationReqdFlag": {
				"type": "string",
